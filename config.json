{
    "inbounds": [
        {
            "port": 23213, //你的中转节点入站端口
            "listen": "0.0.0.0",
            "protocol": "dokodemo-door",
            "settings": {
                "address": "172.31.100.205", //你获取到的节点信息内的境外节点ip
                "port": 2351, //你获取到的节点信息内的境外节点端口
                "network": "tcp,udp"
            },
            "tag": "",
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls"
                ]
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {}
        },
        {
            "protocol": "blackhole",
            "settings": {},
            "tag": "blocked"
        }
    ],
    "routing": {
        "rules": [
            {
                "type": "field",
                "ip": [
                    "geoip:private"
                ],
                "outboundTag": "blocked"
            }
        ]
    }
}
